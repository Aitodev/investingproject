<script>
    $(function () {
        $('#calc-plan').change(function () {
            var item = $(this).find('option:selected');
            var min = item.data('min');
            var max = item.data('max');
            var dep_return = item.data('return');
            var period = item.data('period');
            var days = item.data('days');
            var percent = item.data('percent');

            $('#calc-amount').val(min);

            var daily = min/100*percent;
            if(dep_return==0){
                var total = daily*period;
            }else{
                var total = daily*period+min;
            }

            daily = daily.toFixed(2);
            total = total.toFixed(2);

            $('#calc-daily').text(daily);
            $('#calc-total').text(total);
        });
        $('#calc-amount').on('change keyup',function () {
            var item = $('#calc-plan option:selected');
            var min = item.data('min');
            var max = item.data('max');
            var dep_return = item.data('return');
            var period = item.data('period');
            var days = item.data('days');
            var percent = item.data('percent');
            var amount = parseFloat($(this).val());
            if(amount>max){
                amount = max;
                $(this).val(max);
            }
            if(amount>=min) {

                var daily = amount / 100 * percent;
                if(dep_return==0){
                    var total = daily*period;
                }else{
                    var total = daily*period+amount;
                }

                daily = daily.toFixed(2);
                total = total.toFixed(2);

                $('#calc-daily').text(daily);
                $('#calc-total').text(total);
            }
        });
    });
</script>